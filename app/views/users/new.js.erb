buttonText = document.getElementById("new_team_button").innerHTML
if ( buttonText == "Or create a new team" ) {
  document.getElementById("new_team_button").innerHTML = "Or join existing team"
  $('#user-form').html("<%= j (render 'new_team_form') %>");
} else {
  document.getElementById("new_team_button").innerHTML= "Or create a new team"
  $('#user-form').html("<%= j (render 'existing_team_form') %>");
}
